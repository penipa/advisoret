{
  "expo": {
    "name": "Advisoret",
    "slug": "esmorzapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding/app-icon.png",
    "scheme": "advisoret",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pablope25.advisoret",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#07090D",
        "foregroundImage": "./assets/branding/adaptive-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pablope25.advisoret"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/branding/splash.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#07090D",
          "dark": {
            "backgroundColor": "#07090D"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b17c46b3-29bf-4dd2-bcb6-0afb740b5553"
      }
    }
  }
}
